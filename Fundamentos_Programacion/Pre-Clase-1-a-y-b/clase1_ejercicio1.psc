// Realizar un algoritmo para determinar si un número es positivo o negativo
Proceso clase1_ejercicio1
	Definir num Como Entero;
	Escribir 'Por favor digite un número';
	Leer num;
	Si num>0 Entonces
		Escribir 'El número ', num, ' es positivo';
	SiNo
		Escribir 'El número', num, ' es negativo';
	FinSi
FinProceso
